[
  {
    "name": "cm",
    "title": "Аккорд Мастер",
    "description": "Сборщик Христианских песен для удобного использования",
    "params": [
      "joinedStream:#String:[runtime]",
      "observedComs:#String"
    ],
    "variables": {
      "canRedact": 50,
      "mutedExecs": false
    },
    "requirements": [
      {
        "R": [
          "@prepareActions():1:cm"
        ],
        "name": "actions",
        "ext": "json",
        "track": [
          "@prepareActions()::cm"
        ]
      },
      {
        "path": "~apps/cm/cols.json",
        "file": "cols.json",
        "name": "cols",
        "ext": "json"
      },
      {
        "path": "~apps/cm/meetings.json",
        "file": "meetings.json",
        "name": "meetings",
        "ext": "json"
      },
      {
        "R": [
          "@userLevel",
          ">=",
          50
        ],
        "path": "~apps/cm/e-e-rules.json",
        "file": "e-e-rules.json",
        "name": "eeStorage",
        "ext": "json"
      },
      {
        "R": [
          "@userLevel",
          ">=",
          100
        ],
        "path": "~apps/cm/cm-execs.json",
        "file": "cm-execs.json",
        "name": "execs",
        "ext": "json",
        "track": [
          [
            "~apps/cm/cm-execs.json"
          ],
          {
            "FILTER": [
              "ts",
              ">",
              "@timePrevDay"
            ]
          }
        ]
      },
      {
        "R": [
          "@userLevel",
          ">=",
          100
        ],
        "path": [
          "~apps/cm/cm-execs.json",
          "~S/apps.json"
        ],
        "file": "cm-execs.json",
        "name": "cm_observable_executions",
        "ext": "json",
        "track": [
          [
            "~apps/cm/cm-execs.json"
          ],
          {
            "FILTER": [
              [
                "args",
                "comw"
              ],
              "===",
              20220214131605570
            ]
          }
        ]
      },
      {
        "path": "~apps/cm/chords.json",
        "file": "chords.json",
        "name": "chords",
        "ext": "json"
      },
      {
        "R": [
          "@observeStreamChanges():1:cm"
        ],
        "ext": "json",
        "name": "joined_stream",
        "track": [
          "@observeStreamChanges()::cm"
        ],
        "F": [
          ">joinedStream:cm",
          "is",
          "Truthy"
        ],
        "frequency": 100,
        "comp": true,
        "live": true
      }
    ]
  },
  {
    "name": "admin",
